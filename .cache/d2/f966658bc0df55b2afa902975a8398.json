      return [min].concat(sort(arr))\r\n  }else{\r\n    return arr[0]>arr[1]?arr.reverse():arr\r\n  }\r\n}\r\n\r\n\r\n\r\n//sort 排序 将数组由小到大排列 -- 循环方式\r\n\r\nlet swap =(arr,index,i)=>{\r\n  let temp = arr[index]\r\n  arr[index]= arr[i]\r\n  arr[i]=temp\r\n}\r\n\r\nlet sort2 = arr => {\r\n  for(let i = 0;i<arr.length;i++){\r\n    console.log(`_____`)\r\n    let index = minIndex(arr.slice(i))+i\r\n    console.log(`i:${i}`)\r\n    console.log(`index:${index}`)\r\n    console.log(`min:${arr[index]}`)\r\n    console.log(`_____`)\r\n    if(index!==i){swap(arr,index,i)}\r\n  }\r\n  return arr\r\n}\r\n// sort2(a)\r\n\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"77adf71bdde01e81a8a246c8061c20ac","cacheData":{"env":{}}}